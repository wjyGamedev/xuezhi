<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
              android:id="@+id/select_age_fragment_page">

    <LinearLayout
        android:id="@+id/age_header_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/all_null"
        android:orientation="vertical">

    </LinearLayout>

    <TextView
        android:id="@+id/age_range_0_15_tv"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@drawable/bg_order_btn"
        android:gravity="center"
        android:text="@string/btn_age_section_0_15_text"
        android:textColor="#ff666666"
        android:textSize="14.0sp" />

    <TextView
        android:id="@+id/age_range_16_35_tv"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@drawable/bg_order_btn"
        android:gravity="center"
        android:text="@string/btn_age_section_16_35_text"
        android:textColor="#ff666666"
        android:textSize="14.0sp" />

    <TextView
        android:id="@+id/age_range_36_55_tv"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@drawable/bg_order_btn"
        android:gravity="center"
        android:text="@string/btn_age_section_36_55_text"
        android:textColor="#ff666666"
        android:textSize="14.0sp" />

    <TextView
        android:id="@+id/age_range_56_75_tv"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@drawable/bg_order_btn"
        android:gravity="center"
        android:text="@string/btn_age_section_56_75_text"
        android:textColor="#ff666666"
        android:textSize="14.0sp" />

    <TextView
        android:id="@+id/age_range_above_75_tv"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@drawable/bg_order_btn"
        android:gravity="center"
        android:text="@string/btn_age_section_above_75_text"
        android:textColor="#ff666666"
        android:textSize="14.0sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/mask"
        android:id="@+id/age_bottom_ll"
        android:orientation="vertical">

    </LinearLayout>
</LinearLayout>
